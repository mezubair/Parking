  <script>
    // JavaScript code for toggling the location input mode
    const detectLocationIcon = document.getElementById("detectLocationIcon");
    const locationInput = document.getElementById("locationInput");

    detectLocationIcon.addEventListener("click", function() {
      if (locationInput.disabled) {
        locationInput.disabled = false;
        detectLocationIcon.style.color = "#000"; // Change the icon color to indicate the active state
      } else {
        locationInput.disabled = true;
        detectLocationIcon.style.color = "#007bff"; // Change the icon color to indicate the inactive state
      }
    });
  </script>
