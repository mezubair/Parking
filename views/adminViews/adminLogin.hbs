    <!-- Basic -->
    <div class="ex-form-1 pt-5 pb-5">
        <div class="container">
            <div class="row">
                <div class="col-xl-6 offset-xl-3">
                    <div class="text-box mt-5 mb-5">

                        <!-- Log In Form -->
                        <form id="loginform" action="/adminLogin" method="POST">
                            <div class="mb-4 form-floating">
                                <input type="text" class="form-control" id="ademail" name="ademail">
                                <label for="floatingInput">Username</label>
                                <p class="text-danger" id="floatingInput_error"></p>
                            </div>
                            <div class="mb-4 form-floating">
                                <input type="password" class="form-control" id="adpass"
                                    placeholder="Password" name="adpass">
                                <label for="floatingPassword">Password</label>
                                <p class="text-danger" id="floatingPassword_error"></p>
                            </div>
                        
                            <p class="text-danger" id="exampleCheck1_error"></p>
                            <button type="submit" class="form-control-submit-button">Admin Log in</button>

                           

                        </form>
                        <!-- end of log in form -->

                    </div>
                    <!-- end of text-box -->
                </div>
                <!-- end of col -->
            </div>
